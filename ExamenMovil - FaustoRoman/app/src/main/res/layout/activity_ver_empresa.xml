<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VerEmpresa">

    <ListView
        android:id="@+id/id_list_view_productos"
        android:layout_width="284dp"
        android:layout_height="255dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.313" />

    <Button
        android:id="@+id/id_btn_agregar_producto"
        android:layout_width="153dp"
        android:layout_height="58dp"
        android:layout_marginTop="60dp"
        android:text="Agregar Producto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.864"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_list_view_productos" />

    <Button
        android:id="@+id/id_btn_empresa_atras"
        android:layout_width="118dp"
        android:layout_height="63dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="56dp"
        android:text="VOLVER A EMPRESAS"
        app:layout_constraintBottom_toBottomOf="@+id/id_btn_agregar_producto"
        app:layout_constraintEnd_toStartOf="@+id/id_btn_agregar_producto"
        app:layout_constraintTop_toTopOf="@+id/id_btn_agregar_producto"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>